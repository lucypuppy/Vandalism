{
  "schemaVersion": 1,
  "id": "vandalism",
  "version": "${version}",
  "name": "Vandalism",
  "description": "A mod for Minecraft designed to train server administrators.",
  "authors": [
    "NekosAreKawaii",
    "FooFieOwO",
    "Recyz"
  ],
  "contributors": [
    "Verschlxfene",
    "cuddlycatgirl",
    "nellieldev",
    "xkeksi",
    "NotYourReturn",
    "mori0",
    "richardletshacks",
    "EvilCodeZ",
    "Flauschig",
    "Outfluencer"
  ],
  "environment": "client",
  "license": "GPL-v3",
  "icon": "assets/vandalism/textures/icon/icon_1024x1024.png",
  "entrypoints": {
    "client": [
      "de.nekosarekawaii.vandalism.base.FabricBootstrap"
    ]
  },
  "mixins": [
    "vandalism.mixins.json"
  ],
  "accessWidener": "vandalism.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.3",
    "minecraft": "1.21.1",
    "java": ">=21",
    "fabric-api-base": ">=0.4.35",
    "fabric-resource-loader-v0": ">=0.11.14",
    "fabric-networking-api-v1": ">=3.1.1",
    "fabric-registry-sync-v0": ">=4.0.13",
    "fabric-item-group-api-v1": ">=4.0.20",
    "viafabricplus": ">=3.4.5"
  },
  "custom": {
    "afl:classtransform": "vandalism.classtransform.json",
    "afl:unmixer": {
      "de.florianmichael.viafabricplus.injection.mixin.base.integration": [
        "MixinMultiplayerServerListWidget_ServerEntry"
      ]
    }
  }
}
