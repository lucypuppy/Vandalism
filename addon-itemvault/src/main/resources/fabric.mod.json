{
  "schemaVersion": 1,
  "id": "vandalism-itemvault",
  "version": "${version}",
  "name": "Vandalism-Item-Vault",
  "description": "Implementation of the Item Vault.",
  "authors": [
    "NekosAreKawaii",
    "Verschlxfene"
  ],
  "license": "GPL-v3",
  "environment": "client",
  "entrypoints": {
    "vandalism:onLaunch": [
      "de.nekosarekawaii.vandalism.addonitemvault.AddonItemVault"
    ]
  },
  "accessWidener": "vandalism-itemvault.accesswidener",
  "depends": {
    "vandalism": "*",
    "itemvault": "*",
    "fabric-registry-sync-v0": ">=4.0.13",
    "fabric-item-group-api-v1": ">=4.0.20"
  },
  "custom": {
    "itemvault:disable_inbuilt_integration": true,
    "modmenu": {
      "parent": "vandalism"
    }
  }
}
